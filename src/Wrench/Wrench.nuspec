<?xml version="1.0" encoding="utf-8" ?>
<package>
  <metadata>
    <id>Wrench</id>
    <version>$version$</version>
    <title>Wrench</title>
    <authors>Brad Hess</authors>
    <owners>Brad Hess</owners>
    <licenseUrl>http://www.opensource.org/licenses/mit-license.php</licenseUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Build tools for style and code analysis checking.</description>
    <projectUrl>https://github.com/bdhess/browningstyle</projectUrl>
    <developmentDependency>true</developmentDependency>
    <tags>Build MSBuild StyleCop Code Analysis Coverity</tags>
    <dependencies>
      <group>
        <dependency id="BrowningStyle.MSBuild" version="$version$" />
        <dependency id="FxCopWrapper" version="1.0.6" />
        <dependency id="GitVersionTask" version="2.0.0" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="build\Wrench.props" target="build" />
    <file src="tools\AllRules.ruleset" target="tools" />
    <file src="tools\DefaultSettings.stylecop" target="tools" />
  </files>
</package>
